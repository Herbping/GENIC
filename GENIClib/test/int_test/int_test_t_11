
===
function F (l: Int) : Int :=
	match l with
	| x::y::z::tail when (and (= x 0) (> y 1) (> z 1));
		-> [1; (+ y 1); (+ z 2)] ++ F(tail)
	| x::y::z::tail when (and (= x 1) (> y 2) (> z 2));
		-> [1; (+ y 2); (+ z 3)] ++ F(tail)
	| x::y::z::tail when (and (= x 2) (> y 3) (> z 3));
		-> [1; (+ y 3); (+ z 4)] ++ F(tail)
	| x::y::z::tail when (and (= x 3) (> y 4) (> z 4));
		-> [1; (+ y 4); (+ z 5)] ++ F(tail)
	| x::y::z::tail when (and (= x 4) (> y 5) (> z 5));
		-> [1; (+ y 5); (+ z 6)] ++ F(tail)
	| x::y::z::tail when (and (= x 5) (> y 6) (> z 6));
		-> [1; (+ y 6); (+ z 7)] ++ F(tail)
	| x::y::z::tail when (and (= x 6) (> y 7) (> z 7));
		-> [1; (+ y 7); (+ z 8)] ++ F(tail)
	| x::y::z::tail when (and (= x 7) (> y 8) (> z 8));
		-> [1; (+ y 8); (+ z 9)] ++ F(tail)
	| x::y::z::tail when (and (= x 8) (> y 9) (> z 9));
		-> [1; (+ y 9); (+ z 10)] ++ F(tail)
	| x::y::z::tail when (and (= x 9) (> y 10) (> z 10));
		-> [1; (+ y 10); (+ z 11)] ++ F(tail)
	| x::y::z::tail when (and (= x 10) (> y 11) (> z 11));
		-> [1; (+ y 11); (+ z 12)] ++ F(tail)
===
invert f;
