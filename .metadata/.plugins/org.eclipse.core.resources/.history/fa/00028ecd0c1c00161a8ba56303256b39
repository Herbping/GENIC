package injchecker;

import com.microsoft.z3.*;

public class TransitionInjChecker {
	
	public static boolean check(Context ctx, BoolExpr domain, String[] varsName, Expr[] outputFunc, Sort sort){
		Integer lookahead = varsName.length;
		Expr[] vars = new Expr[lookahead];
		Expr[] boundsA = new Expr[lookahead];
		for(int i = 0; i < lookahead; i++){
			vars[i] = ctx.mkConst(varsName[i], sort); 
			bounds[i] = ctx.mkBound(i, sort);
		}
		BoolExpr notEQ = ctx.mkNot(ctx.mkEq(arg0, arg1))
		return false;
	}
	
}