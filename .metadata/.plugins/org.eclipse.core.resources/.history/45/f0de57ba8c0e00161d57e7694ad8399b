import java.util.Collections;
import java.util.HashMap;
import java.util.HashSet;
import java.util.Map;
import java.util.Set;

import com.microsoft.z3.Expr;
import com.microsoft.z3.Symbol;


	// a interpretation is an (partial) assignment
	public class gInterpretation{
		public final Map<Symbol, Expr> map;
		public gInterpretation(Map<Symbol, Expr> map) {
			this.map = Collections.unmodifiableMap(map);
		}
		@Override
		public String toString() {
			String s = "[ ";
			for(Symbol e: map.keySet()){
				s += e + "->" + map.get(e) + " ";
			}
			return s + "]";
		}
		public gInterpretation remove(Symbol key){
			Map<Symbol, Expr> map = new HashMap<>(this.map);
			map.remove(key);
			return new gInterpretation(map);
		}
	}